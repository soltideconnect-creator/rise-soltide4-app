# Paystack Configuration Example
# Copy this file to .env.local and fill in your values

# Paystack Public Key (Get from https://dashboard.paystack.com/#/settings/developer)
VITE_PAYSTACK_PUBLIC_KEY=pk_live_your_public_key_here

# Premium Price (in kobo - 100 kobo = â‚¦1)
VITE_PREMIUM_PRICE=800000

# Currency
VITE_CURRENCY=NGN

# App URL (Your deployed app URL)
VITE_APP_URL=https://your-app.netlify.app

# Paystack Callback URL
VITE_PAYSTACK_CALLBACK_URL=https://your-app.netlify.app/payment-success

# App ID
VITE_APP_ID=your-app-id

# Environment
VITE_ENV=production
